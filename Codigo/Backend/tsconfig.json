{
  "compilerOptions": {
    "target": "ES2022",                    
    "module": "ESNext",                    
    "moduleResolution": "node",            
    "allowSyntheticDefaultImports": true,  
    "esModuleInterop": true,               
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "./",          
    "outDir": "./build"       
  },
}